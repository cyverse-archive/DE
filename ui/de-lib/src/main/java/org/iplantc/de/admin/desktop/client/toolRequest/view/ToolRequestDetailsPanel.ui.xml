<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
             xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
             xmlns:fields="urn:import:org.iplantc.de.apps.widgets.client.view.editors.widgets"
             xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form">

    <ui:with field="appearance"
             type="org.iplantc.de.admin.desktop.client.toolRequest.ToolRequestView.ToolRequestViewAppearance" />

    <ui:with field="margins"
             type="com.sencha.gxt.core.client.util.Margins">
        <ui:attributes top="0" right="5" bottom="10" left="5"/>
    </ui:with>
    <ui:with field="layoutData"
             type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
        <ui:attributes width="1" height="-1" margins="{margins}"/>
    </ui:with>

    <gxt:ContentPanel headingText="{appearance.detailsPanelHeading}">
        <container:VerticalLayoutContainer ui:field="con">
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.additionalDataFileLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="additionalDataFileEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.additionalInfoLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="additionalInfoEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.architectureLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="architectureEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.attributionLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="attributionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.cmdLineDescriptionLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="cmdLineDescriptionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.documentationUrlLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="documentationUrlEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <fields:CheckBoxAdapter ui:field="multiThreadedEditor"
                               enabled="false"
                               />
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.phoneLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="phoneEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.sourceUrlLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="sourceUrlEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.submittedByLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="submittedByEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.testDataPathLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="testDataPathEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="{appearance.versionLabel}">
                    <form:widget>
                        <form:TextField readOnly="true"
                                        enabled="false"
                                        ui:field="versionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>

        </container:VerticalLayoutContainer>
    </gxt:ContentPanel>
</ui:UiBinder> 
